{
  "extends": ["next/core-web-vitals", "plugin:testing-library/react"],
  "plugins": ["@typescript-eslint"],
  "rules": {
    "@typescript-eslint/no-unused-vars": ["warn", { "args": "none" }],
    "import/no-unresolved": "error",
    "import/named": "off",
    "no-console": "warn",
    "@next/next/no-img-element": "off"
  }
}
